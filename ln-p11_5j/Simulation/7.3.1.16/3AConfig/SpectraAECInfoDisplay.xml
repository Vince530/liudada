          </Row>
          <Row>
            <Value Text="FPS:" Bold="true"/>
            <Value Value="AEC Convergence.Current FPS"/>
          </Row>
          <Row>
            <Value Text="Final Luma:" Bold="true"/>
            <Value Value="AEC FrameHistory[0].Final Luma"/>
          </Row>
          <Row>
            <Value Text="Histogram Enabled:" Bold="true"/>
            <Check Value="AEC Histogram.Histogram Enabled"/>
          </Row>
          <Row>
            <Value Text="ADRC Enabled:" Bold="true"/>
            <Check Value="AEC Histogram.ADRC Enabled"/>
          </Row>
        </Grid>
      </Panel>
