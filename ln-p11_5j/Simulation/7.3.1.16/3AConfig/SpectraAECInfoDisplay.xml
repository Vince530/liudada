      <Panel Condition="{exists}AEC FrameHistory" Title="AEC Overview" Type="Open">
        <Grid>
          <Row>
            <Value Text="" Bold="false"/>
            <Value Text="Short" Bold="true"/>
            <Value Text="Long" Bold="true"/>
            <Value Text="Safe" Bold="true"/>
          </Row>
          <Row>
            <Value Text="Exp. Indices:" Bold="true"/>
            <Value Value="AEC FrameHistory[0].Exposure Indices[0]"/>
            <Value Value="AEC FrameHistory[0].Exposure Indices[1]"/>
            <Value Value="AEC FrameHistory[0].Exposure Indices[2]"/>
          </Row>
          <Row>
            <Value Text="Exp. Target Indices:" Bold="true"/>
            <Value Value="AEC FrameHistory[0].Exposure Target Indices[0]"/>
            <Value Value="AEC FrameHistory[0].Exposure Target Indices[1]"/>
            <Value Value="AEC FrameHistory[0].Exposure Target Indices[2]"/>
          </Row>
          <Row>
            <Value Text="Gains:" Bold="true"/>
            <Value Value="AEC FrameHistory[0].Exposure Gains[0]"/>
            <Value Value="AEC FrameHistory[0].Exposure Gains[1]"/>
            <Value Value="AEC FrameHistory[0].Exposure Gains[2]"/>
          </Row>
          <Row>
            <Value Text="Exposure Time [ms]:" Bold="true"/>
            <Value A="AEC FrameHistory[0].Exposure Times[0]" Value="(A/1000000.0)"/>
            <Value A="AEC FrameHistory[0].Exposure Times[1]" Value="(A/1000000.0)"/>
            <Value A="AEC FrameHistory[0].Exposure Times[2]" Value="(A/1000000.0)"/>
          </Row>
          <Row>
            <Value Text="Lux Index:" Bold="true"/>
